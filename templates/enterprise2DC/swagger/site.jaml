            items:
              $ref: "#/definitions/FailDataNbiSubnetModel"
            description: the failed list.

  NbiSubnetModel:
    allOf:
      - $ref: '#/definitions/BaseServiceModel'
      - type: object
        properties:
          siteId:
            type: string
            description: site id
          cidrBlock:
            type: string
            description: cidr block
          cidrBlockSize:
            type: integer
            format: int32
            description: cidr block size 8~32
          gatewayIp:
            type: string
            description: gateway ip
          vlanId:
            type: string
            description: vlan id
          vni:
            type: string
            description: vni
          ports:
            type: array
            items:
              type: string
            description: ports list
          portNames:
            type: array
            items:
              type: string
            description: port name list
          ipv6Address:
            type: string
            description: ipv6 address
          prefixLength:
            type: string
            description: length of prifix
          dhcp6Enable:
            type: string
            description: whether enable dhcp6
          dhcp6Mode:
