definitions:
  UuidModel:
    type: object
    properties:
      id:
        type: string
        description: the UUID of the model

  ModelBase:
    allOf:
      - $ref: '#/definitions/UuidModel'
      - type: object
        required:
          - tenantId
        properties:
          name:
            type: string
            description: the name of the model
          tenantId:
            type: string
            description: tenant ID
          description:
            type: string
            description: description info
          additionalInfo:
            type: array
            items:
              $ref: '#/definitions/NvString'
            description: additional info
  NvString:
    allOf:
      - $ref: '#/definitions/UuidModel'
      - type: object
        required:
          - name
        properties:
          name:
            type: string
            description: name, 1 <= length <= 36
          value:
            type: string
            description: value, min = 0, max =255
          firstParentUuid:
            type: string
            description: uuid, 1 <= length <= 36

  BaseServiceModel:
    allOf:
      - $ref: '#/definitions/ModelBase'
      - type: object
        required:
          - adminStatus
        properties:
          adminStatus:
            type: string
            description:  admin state (scope = none, active, inactive, partially_inactive)
          operStatus:
            type: string
            description: (scope = none, up, down, partially_down)
          actionState:
            type: string
            description: (scope = none, normal, creating, deleting, updating, create_exception, update_exception, delete_exception, deploying, deploy_exception, undeploying, undeploy_exception, checking, check_exception)
          createTime:
            type: string
            description: create time
  ErrorCodeInfo:
    type: object
    description: Error code information model
    required:
      - errorCode
      - description
      - objectId
      - objectName
      - object
      - descArg
      - reasonArg
      - detailArg
    properties:
      errorCode:
        type: string
        description: error code
      description:
        type: string
        description: error description
      objectId:
        type: string
        description: object Id
      objectName:
        type: string
        description: object Name
      object:
        type: object
        description: object
      descArg:
        type: string
        description: The description args
      reasonArg:
        type: string
        description: The reason args
      detailArg:
        type: string
        description: The detail args
  ResultResponse:
    type: object
    description: Result response model
    required:
      - errorCode
      - httpCode
    properties:
      errorCode:
        type: string
        description: result error code
      descArg:
        type: string
        description: The description args
      reasonArg:
        type: string
        description: The reason args
      detailArg:
        type: string
        description: The detail args
      adviceArg:
        type: string
        description: The advice args
      httpCode:
        type: integer
        description: The http status code
      message:
        type: string
        description: the exception message word
      smallErrorCodeList:
        $ref: "#/definitions/ErrorCodeInfo"

  FailDataNbiVlanModel:
    type: object
    description: Fail data model
    properties:
      errcode:
        type: string
        description: result error code
      errmsg:
        type: string
        description: error message
      data:
        $ref: "#/definitions/NbiVlanModel"
  NbiVlanModelRsp:
    description: NbiVlanModel result model
    allOf:
      - $ref: '#/definitions/ResultResponse'
      - type: object
        properties:
          data:
            $ref: "#/definitions/NbiVlanModel"
          successed:
            type: array
            items:
              $ref: "#/definitions/NbiVlanModel"
            description: the successed list.
          fail:
            type: array
            items:
              $ref: "#/definitions/FailDataNbiVlanModel"
            description: the failed list.
  NbiVlanModel:
    allOf:
      - $ref: '#/definitions/BaseServiceModel'
      - type: object
        properties:
          siteId:
            type: string
            description: site id
          vlanId:
            type: integer
            format: int32
            description: vlan id
          ports:
            type: array
            items:
              type: string
            description: ports list
          portNames:
            type: array
            items:
              type: string
            description: port name list
          createtime:
            type: integer
            format: int64
            description: create time
          updatetime:
            type: integer
            format: int64
            description: update time

  FailDataNbiSubnetModel:
    type: object
    description: Fail data model
    properties:
      errcode:
        type: string
        description: result error code
      errmsg:
        type: string
        description: error message
      data:
        $ref: "#/definitions/NbiSubnetModel"

  NbiSubnetModelRsp:
    description: NbiSubnetModel result model
    allOf:
      - $ref: '#/definitions/ResultResponse'
      - type: object
        properties:
          data:
            $ref: "#/definitions/NbiSubnetModel"
          successed:
            type: array
            items:
              $ref: "#/definitions/NbiSubnetModel"
            description: the successed list.
          fail:
